<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUp">



    <TextView
        android:id="@+id/SignIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="106dp"
        android:layout_marginBottom="92dp"
        android:clickable="true"
        android:text="Sign In."
        android:textColor="#50ABF1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logInBu_2"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/haveacc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="107dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="92dp"
        android:text="Already have account ?"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logInBu_2" />

    <Button
        android:id="@+id/SignUpBu_reg"
        android:layout_width="159dp"
        android:layout_height="49dp"
        android:layout_marginStart="126dp"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="126dp"
        android:layout_marginBottom="8dp"
        android:backgroundTint="#50ABF1"
        android:text="Sign Up"
        app:layout_constraintBottom_toTopOf="@+id/SignIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/enterPassword_reg"
        tools:ignore="TextContrastCheck" />

    <EditText
        android:id="@+id/enterName"
        android:layout_width="289dp"
        android:layout_height="43dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="61dp"
        android:ems="10"
        android:hint="Name"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/twitter2"
        tools:ignore="TouchTargetSizeCheck,DuplicateClickableBoundsCheck" />

    <EditText
        android:id="@+id/enterEmail"
        android:layout_width="289dp"
        android:layout_height="43dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="61dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enterName"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterMobile"
        android:layout_width="289dp"
        android:layout_height="43dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="61dp"
        android:ems="10"
        android:hint="Mobile Number"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enterEmail"
        tools:ignore="TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/enterPassword_reg"
        android:layout_width="289dp"
        android:layout_height="43dp"
        android:layout_marginStart="61dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="61dp"
        android:ems="10"
        android:hint="Password"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enterMobile"
        tools:ignore="TouchTargetSizeCheck" />



    <TextView
        android:id="@+id/twitter2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="37dp"
        android:layout_marginTop="11dp"
        android:layout_marginEnd="231dp"
        android:layout_marginBottom="473dp"
        android:text="Please enter details to register"
        android:textColor="#50ABF1"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SignUp" />

    <TextView
        android:id="@+id/SignUp"
        android:layout_width="0dp"
        android:layout_height="63dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="57dp"
        android:layout_marginEnd="118dp"
        android:layout_marginBottom="511dp"
        android:text="Register"
        android:textColor="#50ABF1"
        android:textSize="45sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo2" />

    <ImageView
        android:id="@+id/logo2"
        android:layout_width="54dp"
        android:layout_height="63dp"
        android:layout_marginTop="37dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.098"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_twitter_seeklogo_com" />

</androidx.constraintlayout.widget.ConstraintLayout>